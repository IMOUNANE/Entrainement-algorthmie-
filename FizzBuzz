
for(let i=0;i<=101;i++){
	//test variable modulo 3 puis modulo 5  et enfin les deux Ã  la fois
    if(i % 3 === 0){
        console.log("fizz" );
    }else if(i % 5 === 0){
        console.log("buzz");
    }else if(i % 3===0 && i %5 ===0){
        console.log("fizzbuzz");
    }else{
        console.log(i);
    }
}

